<?php
$prd = $block->getProduct();
$attributeSetId = $prd->getAttributeSetId();


$objectManager = \Magento\Framework\App\ObjectManager::getInstance();
$config  = $objectManager->get('Magento\Catalog\Model\Config');
$attributeGroupId = $config->getAttributeGroupId($attributeSetId, 'Migration_Custom');

$productAttributes=$prd->getAttributes();
foreach ($productAttributes as $attribute) {
	if ($attribute->isInGroup($attributeSetId, $attributeGroupId)) {

		if ($attribute->getAttributeCode() == 'status' || $attribute->getAttributeCode() == 'brand_code' || $attribute->getAttributeCode() == 'cost_price' || $attribute->getAttributeCode() == 'tax_class_id' || $attribute->getAttributeCode() == 'bulb_included_2' || $attribute->getAttributeCode() == 'bulb_number_2' || $attribute->getAttributeCode() == 'bulb_type_2' || $attribute->getAttributeCode() == 'bulb_wattage_2' )
		{
			continue;
		}

		if ($attribute->getAttributeCode() == 'style' && $attribute->getFrontend()->getValue($prd) == 'No')
			continue;


		if ($attribute->getFrontend()->getValue($prd) == '' || $attribute->getFrontend()->getValue($prd) == '0')
			continue;




		echo '<div class="dl-specification-heading">'.$attribute->getFrontendLabel().'</div><div class="dl-specification-value">'.$attribute->getFrontend()->getValue($prd).'</div>';
			
        //echo $attribute->getFrontendLabel().' : '.$attribute->getFrontend()->getValue($prd).' : '.$attribute->getAttributeCode().'<br />';

	}
}


?>

</div>


